from pingshan.models import Appeal
import json
with open("C:\\Users\\Xin\\Desktop\\testdj 2\\testdj\\shujuju.json", encoding='utf-8') as f:
    line = f.readline()
    d = json.loads(line)
    num = 0
    for record in d:
        new = {}
        for var in [*record]:
            if record[var] != '-':
                new[var] = record[var]
        tmp = Appeal(**new)
        tmp.save()
        num += 1
        print(num)






















